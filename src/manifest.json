{
  "name": "TrelloBurnDownChromeExtension",
  "description": "Trelloで使える手軽なバーンダウンチャートです",
  "version": "0.1.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "19": "image/icon-16.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "background",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+B",
        "mac": "Command+Shift+B"
      }
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'unsafe_inline'; object-src 'self'"
}
